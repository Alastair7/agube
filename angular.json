{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "agube-fe": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/agube-fe",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "allowedCommonJsDependencies": ["lodash"],
                        "styles": [
                            "src/styles.scss",
                            "node_modules/@fortawesome/fontawesome-free/css/all.css"
                        ],
                        "scripts": [
                        ],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "180kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "agube-fe:build","proxyConfig": "src/proxy.config.js"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "agube-fe:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "agube-fe:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "tsconfig.app.json",
                            "tsconfig.spec.json",
                            "e2e/tsconfig.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "agube-fe:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "agube-fe:serve:production"
                        }
                    }
                }
            }
        },
        "auth-rest-api-lib": {
            "projectType": "library",
            "root": "projects/auth-rest-api-lib",
            "sourceRoot": "projects/auth-rest-api-lib/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/auth-rest-api-lib/tsconfig.lib.json",
                        "project": "projects/auth-rest-api-lib/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/auth-rest-api-lib/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/auth-rest-api-lib/src/test.ts",
                        "tsConfig": "projects/auth-rest-api-lib/tsconfig.spec.json",
                        "karmaConfig": "projects/auth-rest-api-lib/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/auth-rest-api-lib/tsconfig.lib.json",
                            "projects/auth-rest-api-lib/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "task-rest-api-lib": {
            "projectType": "library",
            "root": "projects/task-rest-api-lib",
            "sourceRoot": "projects/task-rest-api-lib/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/task-rest-api-lib/tsconfig.lib.json",
                        "project": "projects/task-rest-api-lib/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/task-rest-api-lib/tsconfig.lib.prod.json"
                        }
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/task-rest-api-lib/src/test.ts",
                        "tsConfig": "projects/task-rest-api-lib/tsconfig.spec.json",
                        "karmaConfig": "projects/task-rest-api-lib/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "projects/task-rest-api-lib/tsconfig.lib.json",
                            "projects/task-rest-api-lib/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "agube-fe"
}
